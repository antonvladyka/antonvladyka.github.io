function checkRise(data){
	var str = data ? 'up' : 'down';
	return str;
}
function generateHtml(data){
	var str = '<a href="http://www.finam.ru/?utm_source=r_today&utm_medium=promo_logo&utm_campaign=brending_finam" target="_blank"><img src="/net/imageheap/widget/RT/logo-finam.png"/> ';
	var strD = '<span class="item">&#36; ' + data.QuotesItems[0].ClosePrice + '<img src="/net/imageheap/widget/RT/' + checkRise(data.QuotesItems[0].PriceRise) + '.png"/></span>';
	var strE = '<span class="item">&#8364; ' + data.QuotesItems[1].ClosePrice + '<img src="/net/imageheap/widget/RT/' + checkRise(data.QuotesItems[1].PriceRise) + '.png"/></span>';
	str += strD + strE + '</a>';
	return str;
}
$(document).ready(function(){
	var html = generateHtml(data);
	$('#finam_rt_widget').html(html)
});
